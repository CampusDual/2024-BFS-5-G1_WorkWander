<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
                 catalog="" schema="${mainschema}" table="booking" datasource="mainDataSource"
                 sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>bk_id</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>bk_id</Column>
    </UpdateKeys>
    <GeneratedKey>bk_id</GeneratedKey>
    <Queries>
        <Query id="totalBookingsByDateAndCoworking">
            <Sentence>
                <![CDATA[
                    SELECT COUNT(b.bk_id) as "plazasOcupadas"
                    FROM ${mainschema}.booking b
                    join ${mainschema}.booking_date bd
                    on b.bk_id=bd.bk_id
                    #WHERE#
                ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>