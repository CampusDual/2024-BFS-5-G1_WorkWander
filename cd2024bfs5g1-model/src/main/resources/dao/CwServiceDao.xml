<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
                 catalog="" schema="${mainschema}" table="cw_service" datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>cser_id</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>cser_id</Column>
    </UpdateKeys>
    <GeneratedKey>cser_id</GeneratedKey>
    <Queries>
        <Query id="servicePerCoworking">
            <Sentence>
                <![CDATA[SELECT srv_name as servicios FROM ${mainschema}.cw_service as cws JOIN ${mainschema}.service as s ON cws.srv_id = s.srv_id #WHERE# ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>